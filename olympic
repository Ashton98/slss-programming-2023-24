finalscore = 0
judge_score = ["Judge 1:", "Judge 2:", "Judge 3:", "Judge 4:", "Judge 5:"]
for judge in judge_score:
    print(judge)
    score = float(input().strip("!,.?"))
    if score < 0 or score > 10:
        break
    else:
        finalscore += score
print(round (("Your olympic score is", finalscore / 5), 2))
